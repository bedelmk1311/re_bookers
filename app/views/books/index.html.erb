<h1>Books</h1>
<% @books.each do |book|%>
  <table>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.body %></td>
      <td><%= link_to "仮Show"     %></td>
      <td><%= link_to "仮Edit"     %></td>
      <td><%= link_to "仮Destroy"     %></td>
    </tr>
  </table>
<% end %>
